  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch with Instana enabled",
      "request": "launch",
      "runtimeArgs": ["run", "instanaEnabled"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "runtimeVersion": "12"
    },
    {
      "name": "Launch with Instana enabled and disabled traces",
      "request": "launch",
      "runtimeArgs": ["run", "instanaEnabledWithDisabledTrace"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "runtimeVersion": "12"
    },
    {
      "name": "Launch with Instana disabled",
      "request": "launch",
      "runtimeArgs": ["run", "instanaDisabled"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outputCapture": "std",
      "runtimeVersion": "12"
    }
  ]
}
